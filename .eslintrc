{
  "extends": ["recommended/esnext", "plugin:flowtype/recommended"],
  "rules": {
    "max-len": [2, 120, {"ignoreUrls": true, "ignoreStrings": true, "ignoreTemplateLiterals": true}],
    "import/prefer-default-export": 0,
    "import/no-nodejs-modules": 0,
    "class-methods-use-this": 0,
    "no-duplicate-imports": 0
  },
  "plugins": ["jest", "flowtype"],
  "parser": "babel-eslint",
  "env": {
    "jest": true
  }
}
